{
  "name": "fennek",
  "version": "0.0.1",
  "description": "A CMS with its own language built in",
  "license": "NONE",
  "repository": {
    "type": "git",
    "url": "https://github.com/ewert-online/fennek"
  },
  "author": { "name": "Torben Ewert", "email": "torben@ewert-online.com" },
  "bugs": { "url": "https://github.com/ewert-online/fennek/issues" },
  "keywords": [],
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildDev": "dune build --promote-install-files --root . --only-package #{self.name}"
  },
  "scripts": {
    "test": "esy x dune runtest",
    "test:u": "esy x dune promote"
  },
  "dependencies": {
    "@opam/menhir": "*",
    "@opam/ocamlformat": "0.20.1",
    "@opam/ocamlformat-rpc": "0.20.1",
    "@opam/ppx_deriving": "*",
    "@opam/re": "*",
    "@opam/uucp": "*",
    "@opam/uutf": "*"
  },
  "devDependencies": {
    "@opam/dune": "< 3.0.0",
    "@opam/ocaml-lsp-server": "1.9.0",
    "ocaml": "4.12.0"
  }
}
