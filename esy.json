{
  "name": "pinc-lang",
  "version": "0.0.1",
  "description": "A CMS with its own language built in",
  "license": "NONE",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinc-lang/pinc-lang"
  },
  "author": { "name": "Torben Ewert", "email": "torben@ewert-online.com" },
  "bugs": { "url": "https://github.com/pinc-lang/pinc-lang/issues" },
  "keywords": [],
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildDev": "dune build --promote-install-files --root . --only-package #{self.name}"
  },
  "scripts": {
    "t": "esy x dune runtest",
    "tu": "esy x dune promote"
  },
  "dependencies": {
    "@opam/fileutils": "0.6.3",
    "@opam/iter": "1.3"
  },
  "devDependencies": {
    "@opam/dune": "< 3.0.0",
    "@opam/ocaml-lsp-server": "1.9.1",
    "@opam/ocamlformat": "0.20.1",
    "ocaml": "4.13.1000"
  },
  "resolutions": {
    "ocaml": {
      "version": "4.13.1000",
      "override": {
        "build": ["./esy-configure --enable-flambda --prefix $cur__install", "./esy-build"]
      }
    }
  }
}
