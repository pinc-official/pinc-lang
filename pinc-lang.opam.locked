  "uuseg" {= "15.1.0" & with-dev-setup}
  "uutf" {= "1.0.3" & with-dev-setup}
  "xdg" {= "3.12.1" & with-dev-setup}
  "yojson" {= "2.1.2" & with-dev-setup}
]
build: [
  ["dune" "subst"] {dev}
  [
    "dune"
    "build"
    "-p"
    name
    "-j"
    jobs
    "@install"
    "@runtest" {with-test}
    "@doc" {with-doc}
  ]
]
dev-repo: "git+https://github.com/pinc-lang/pinc-lang.git"