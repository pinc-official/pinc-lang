component Component(
  label: "Section",
  icon: "/images/icons/group.svg",
  group: "Structure",
) {
  let array = for (i in 0..10) { i };
  let record = { a: 10, c: "Something" };

  <SlotProvider>
    <SlotProvider.Default>
      <div>Hello Default</div>
    </SlotProvider.Default>

    <SlotProvider.Restricted>
      <SubComponent
        content="Hello, SubComponent!"
        num={3}
        array_things={array}
        record={record}
      ></SubComponent>
    </SlotProvider.Restricted>

    <SlotProvider.Nothing></SlotProvider.Nothing>
  </SlotProvider>
}
